<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://j2ee.netbeans.org/wsdl/Bypass/src/DescriptorWSDL" xmlns:ns2="http://enterprise.netbeans.org/bpel/Bypass/bypass" xmlns:ns3="http://web/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>BypassCA</name>
            <description>Represents the Service Assembly of BypassCA</description>
        </identification>
        <service-unit>
            <identification>
                <name>BypassCA-Bypass</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>Bypass.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>BypassCA-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="DescriptorWSDLPort" service-name="ns1:DescriptorWSDLService"/>
                <provider endpoint-name="DescriptorWSDLPortTypeRole_myRole" service-name="ns2:PartnerLink1"/>
            </connection>
            <connection>
                <consumer endpoint-name="WSPagosRole_partnerRole" service-name="ns2:Pago"/>
                <provider endpoint-name="WSPagosPort" service-name="ns3:WSPagos"/>
            </connection>
        </connections>
        <route-types xmlns="http://www.open-esb.net/xml/ns/jbi">
            <source endpoint-name="DescriptorWSDLPort" mode="local" routing-rule="default" service-name="ns1:DescriptorWSDLService">direct</source>
            <source endpoint-name="WSPagosRole_partnerRole" mode="local" routing-rule="default" service-name="ns2:Pago">direct</source>
        </route-types>
        <interface-mapping xmlns="http://www.open-esb.net/xml/ns/jbi">
            <implementation>
                <by-provider endpoint-name="DescriptorWSDLPortTypeRole_myRole" service-name="ns2:PartnerLink1"/>
                <interface name="ns1:DescriptorWSDLPortType"/>
            </implementation>
            <implementation>
                <by-provider endpoint-name="WSPagosPort" service-name="ns3:WSPagos"/>
                <interface name="ns3:WSPagos"/>
            </implementation>
        </interface-mapping>
    </service-assembly>
</jbi>
