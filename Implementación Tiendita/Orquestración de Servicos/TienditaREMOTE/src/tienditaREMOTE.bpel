<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="tiendita"
    targetNamespace="http://enterprise.netbeans.org/bpel/Tiendita/tiendita"
    xmlns:tns="http://enterprise.netbeans.org/bpel/Tiendita/tiendita"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling">
    <import namespace="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" location="DescriptorWSDL/TienditaWSDL.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/WSEnviosWrapper" location="WSEnviosWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://web/" location="http://localhost:9090/WSEnvios/WSEnvios?WSDL" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/WSOrdenCompraWrapper" location="WSOrdenCompraWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://web/" location="http://localhost:9090/WSOrdenCompra/WSOrdenCompra?WSDL" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/WSAlmacenWrapper" location="WSAlmacenWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://web/" location="http://localhost:9090/WSAlmacen/WSAlmacen?WSDL" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/WSPagosWrapper" location="WSPagosWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://web/" location="http://localhost:9090/WSPagos/WSPagos?WSDL" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://wsalmacen/" location="http://23.96.101.51:8080/WSAlmacen/WSAlmacen?WSDL" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://wsenvios/" location="Envio/23.96.101.51_8080/WSEnvios/WSEnvios.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://wsordencompra/" location="http://23.96.101.51:8080/WSOrdenCompra/WSOrdenCompra?WSDL" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/WSPagosConTDCWrapper" location="WSPagosConTDCWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://wspagoscontdc/" location="http://23.96.101.51:8080/WSPagosConTDC/WSPagosConTDC?WSDL" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="Tienda" xmlns:tns="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" partnerLinkType="tns:TienditaWSDL" myRole="TienditaWSDLPortTypeRole"/>
        <partnerLink name="Almacen" xmlns:tns="http://enterprise.netbeans.org/bpel/WSAlmacenWrapper" partnerLinkType="tns:WSAlmacenLinkType" partnerRole="WSAlmacenRole"/>
        <partnerLink name="Envios" xmlns:tns="http://enterprise.netbeans.org/bpel/WSEnviosWrapper" partnerLinkType="tns:WSEnviosLinkType" partnerRole="WSEnviosRole"/>
        <partnerLink name="Orden" xmlns:tns="http://enterprise.netbeans.org/bpel/WSOrdenCompraWrapper" partnerLinkType="tns:WSOrdenCompraLinkType" partnerRole="WSOrdenCompraRole"/>
        <partnerLink name="Pagos" xmlns:tns="http://enterprise.netbeans.org/bpel/WSPagosConTDCWrapper" partnerLinkType="tns:WSPagosConTDCLinkType" partnerRole="WSPagosConTDCRole"/>
    </partnerLinks>
    <variables>
        <variable name="AltaSolicitudDeEnvioOut1" xmlns:tns="http://wsenvios/" messageType="tns:altaSolicitudDeEnvioResponse"/>
        <variable name="AltaSolicitudDeEnvioIn1" xmlns:tns="http://wsenvios/" messageType="tns:altaSolicitudDeEnvio"/>
        <variable name="ReverseaDescuentaExistenciaOut1" xmlns:tns="http://wsalmacen/" messageType="tns:ReverseaDescuentaExistenciaResponse"/>
        <variable name="ReverseaDescuentaExistenciaIn1" xmlns:tns="http://wsalmacen/" messageType="tns:ReverseaDescuentaExistencia"/>
        <variable name="AltaOCOut1" xmlns:tns="http://wsordencompra/" messageType="tns:altaOCResponse"/>
        <variable name="AltaOCIn1" xmlns:tns="http://wsordencompra/" messageType="tns:altaOC"/>
        <variable name="MontoVtayDescuentaExistenciaOut1" xmlns:tns="http://wsalmacen/" messageType="tns:MontoVtayDescuentaExistenciaResponse"/>
        <variable name="PagoConTDCOut1" xmlns:tns="http://wspagoscontdc/" messageType="tns:PagoConTDCResponse"/>
        <variable name="PagoConTDCIn1" xmlns:tns="http://wspagoscontdc/" messageType="tns:PagoConTDC"/>
        <variable name="MontoVtayDescuentaExistenciaIn1" xmlns:tns="http://wsalmacen/" messageType="tns:MontoVtayDescuentaExistencia"/>
        <variable name="TienditaWSDLOperationOut2" xmlns:tns="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" messageType="tns:TienditaWSDLOperationResponse"/>
        <variable name="AltaSolicitudDeEnvioOut" xmlns:tns="http://web/" messageType="tns:altaSolicitudDeEnvioResponse"/>
        <variable name="AltaSolicitudDeEnvioIn" xmlns:tns="http://web/" messageType="tns:altaSolicitudDeEnvio"/>
        <variable name="TienditaWSDLOperationOut1" xmlns:tns="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" messageType="tns:TienditaWSDLOperationResponse"/>
        <variable name="ReverseaDescuentaExistenciaOut" xmlns:tns="http://web/" messageType="tns:ReverseaDescuentaExistenciaResponse"/>
        <variable name="ReverseaDescuentaExistenciaIn" xmlns:tns="http://web/" messageType="tns:ReverseaDescuentaExistencia"/>
        <variable name="AltaOCOut" xmlns:tns="http://web/" messageType="tns:altaOCResponse"/>
        <variable name="AltaOCIn" xmlns:tns="http://web/" messageType="tns:altaOC"/>
        <variable name="TienditaWSDLOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" messageType="tns:TienditaWSDLOperationResponse"/>
        <variable name="PagoConTDCOut" xmlns:tns="http://web/" messageType="tns:PagoConTDCResponse"/>
        <variable name="PagoConTDCIn" xmlns:tns="http://web/" messageType="tns:PagoConTDC"/>
        <variable name="MontoVtayDescuentaExistenciaOut" xmlns:tns="http://web/" messageType="tns:MontoVtayDescuentaExistenciaResponse"/>
        <variable name="MontoVtayDescuentaExistenciaIn" xmlns:tns="http://web/" messageType="tns:MontoVtayDescuentaExistencia"/>
        <variable name="TienditaWSDLOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" messageType="tns:TienditaWSDLOperationRequest"/>
    </variables>
    <sequence>
        <receive name="RequestCT" createInstance="yes" partnerLink="Tienda" operation="TienditaWSDLOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" portType="tns:TienditaWSDLPortType" variable="TienditaWSDLOperationIn"/>
        <assign name="AStock">
            <copy>
                <from variable="TienditaWSDLOperationIn" part="PRODUCT_ID"/>
                <to>$MontoVtayDescuentaExistenciaIn.parameters/Product_Id</to>
            </copy>
            <copy>
                <from variable="TienditaWSDLOperationIn" part="QUANTITY"/>
                <to>$MontoVtayDescuentaExistenciaIn.parameters/Cantidad</to>
            </copy>
        </assign>
        <invoke name="MontoQTY" partnerLink="Almacen" operation="MontoVtayDescuentaExistencia" xmlns:tns="http://wsalmacen/" portType="tns:WSAlmacen" inputVariable="MontoVtayDescuentaExistenciaIn1" outputVariable="MontoVtayDescuentaExistenciaOut1"/>
        <if name="Existencia">
            <condition>0 &gt; $MontoVtayDescuentaExistenciaOut.parameters/return</condition>
            <sequence name="BuscaTC">
                <assign name="APago">
                    <copy>
                        <from>100000 + $TienditaWSDLOperationIn.CUSTOMER_ID</from>
                        <to>$PagoConTDCIn.parameters/intTDC</to>
                    </copy>
                    <copy>
                        <from>$MontoVtayDescuentaExistenciaOut.parameters/return</from>
                        <to>$PagoConTDCIn.parameters/dblMonto</to>
                    </copy>
                </assign>
                <invoke name="pagoTDC" partnerLink="Pagos" operation="PagoConTDC" xmlns:tns="http://wspagoscontdc/" portType="tns:WSPagosConTDC" inputVariable="PagoConTDCIn1" outputVariable="PagoConTDCOut1"/>
                <if name="Pago">
                    <condition>0 &gt; $PagoConTDCOut.parameters/return</condition>
                    <sequence name="Sequence4">
                        <assign name="AOrden">
                            <copy>
                                <from variable="TienditaWSDLOperationIn" part="CUSTOMER_ID"/>
                                <to>$AltaOCIn.parameters/Customer_Id</to>
                            </copy>
                            <copy>
                                <from variable="TienditaWSDLOperationIn" part="PRODUCT_ID"/>
                                <to>$AltaOCIn.parameters/Product_Id</to>
                            </copy>
                            <copy>
                                <from variable="TienditaWSDLOperationIn" part="QUANTITY"/>
                                <to>$AltaOCIn.parameters/Quantity</to>
                            </copy>
                            <copy>
                                <from>25</from>
                                <to>$AltaOCIn.parameters/ShippingCost</to>
                            </copy>
                            <copy>
                                <from>1</from>
                                <to>$AltaOCIn.parameters/intSalesDate</to>
                            </copy>
                            <copy>
                                <from>0</from>
                                <to>$AltaOCIn.parameters/intShippingDate</to>
                            </copy>
                            <copy>
                                <from>'DHL'</from>
                                <to>$AltaOCIn.parameters/FreigthCompany</to>
                            </copy>
                        </assign>
                        <invoke name="RegistroOC" partnerLink="Orden" operation="altaOC" xmlns:tns="http://wsordencompra/" portType="tns:WSOrdenCompra" inputVariable="AltaOCIn1" outputVariable="AltaOCOut1"/>
                        <assign name="AEnvio">
                            <copy>
                                <from>$AltaOCOut.parameters/return</from>
                                <to>$AltaSolicitudDeEnvioIn.parameters/Orden_Compra</to>
                            </copy>
                            <copy>
                                <from variable="TienditaWSDLOperationIn" part="CUSTOMER_ID"/>
                                <to>$AltaSolicitudDeEnvioIn.parameters/Customer_ID</to>
                            </copy>
                            <copy>
                                <from>1</from>
                                <to>$AltaSolicitudDeEnvioIn.parameters/Status</to>
                            </copy>
                        </assign>
                        <invoke name="Envio" partnerLink="Envios" operation="altaSolicitudDeEnvio" xmlns:tns="http://wsenvios/" portType="tns:WSEnvios" inputVariable="AltaSolicitudDeEnvioIn1" outputVariable="AltaSolicitudDeEnvioOut1"/>
                        <reply name="OK" partnerLink="Tienda" operation="TienditaWSDLOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" portType="tns:TienditaWSDLPortType" variable="TienditaWSDLOperationOut2"/>
                    </sequence>
                    <else>
                        <sequence name="Sequence3">
                            <assign name="ARev">
                                <copy>
                                    <from variable="TienditaWSDLOperationIn" part="PRODUCT_ID"/>
                                    <to>$ReverseaDescuentaExistenciaIn.parameters/Product_Id</to>
                                </copy>
                                <copy>
                                    <from variable="TienditaWSDLOperationIn" part="QUANTITY"/>
                                    <to>$ReverseaDescuentaExistenciaIn.parameters/Cantidad</to>
                                </copy>
                            </assign>
                            <invoke name="Incrementa" partnerLink="Almacen" operation="ReverseaDescuentaExistencia" xmlns:tns="http://wsalmacen/" portType="tns:WSAlmacen" inputVariable="ReverseaDescuentaExistenciaIn1" outputVariable="ReverseaDescuentaExistenciaOut1"/>
                            <assign name="AResp">
                                <copy>
                                    <from>'NO HAY CREDITO'</from>
                                    <to variable="TienditaWSDLOperationOut1" part="RESPONSE"/>
                                </copy>
                            </assign>
                            <reply name="NoCredito" partnerLink="Tienda" operation="TienditaWSDLOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" portType="tns:TienditaWSDLPortType" variable="TienditaWSDLOperationOut1"/>
                        </sequence>
                    </else>
                </if>
            </sequence>
            <else>
                <sequence name="Sequence1">
                    <assign name="NOQTY">
                        <copy>
                            <from>'PRODUCTO AGOTADO'</from>
                            <to variable="TienditaWSDLOperationOut" part="RESPONSE"/>
                        </copy>
                    </assign>
                    <reply name="SinQTY" partnerLink="Tienda" operation="TienditaWSDLOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/Tiendita/DescriptorWSDL/TienditaWSDL" portType="tns:TienditaWSDLPortType" variable="TienditaWSDLOperationOut"/>
                </sequence>
            </else>
        </if>
    </sequence>
</process>
